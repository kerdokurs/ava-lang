loc std::sample;

const a = 1;

fun test(a: u8) -> void {
    if a == 0 {
        Print(a+1);
    } else {
        Print(a);
        a = 5;
        Print(a);
    }

    var n = 0;
    while n < 5 {
        Print("n:");
        Print(n);
        n = n + 1;
    }
}

fun main() -> void {
    test(10);
    test(0);
    Print(a);

    var inp = Input();
    Print(inp);
}